{
  "name": "@streaming/cam-health",
  "version": "0.0.0",
  "description": "runs the show",
  "main": "",
  "bin": "./dist/index.js",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "pkg": "pkg . --out-path ./dist/bin --targets node8-linux-x64",
    "build": "rimraf dist && cross-env NODE_ENV=production tsc",
    "dev": "cross-env NODE_ENV=development nodemon --watch ./ --exec ts-node src/index.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@streaming/types": "file:../types",
    "body-parser": "1.18.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "express": "4.16.2",
    "fs-extra": "4.0.2",
    "morgan": "1.9.0",
    "node-fetch": "1.7.3",
    "nodemon": "1.12.1",
    "nsqjs": "0.9.2",
    "serve-index": "1.9.1",
    "winston": "2.4.0",
    "winston-daily-rotate-file": "1.7.2",
    "ws": "3.3.1"
  },
  "devDependencies": {
    "@types/express": "4.0.39",
    "@types/fs-extra": "4.0.5",
    "@types/node": "8.0.53",
    "@types/node-fetch": "1.6.7",
    "@types/nsqjs": "0.9.0",
    "@types/serve-index": "1.7.29",
    "@types/winston": "2.3.7",
    "@types/ws": "3.2.0",
    "commitizen": "2.9.6",
    "cross-env": "5.1.1",
    "ncp": "2.0.0",
    "pkg": "4.3.0-beta.1",
    "rimraf": "2.6.2",
    "ts-node": "3.3.0",
    "typescript": "2.6.1"
  }
}
