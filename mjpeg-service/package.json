{
  "name": "@streaming/mjpeg-service",
  "version": "0.6.0",
  "description": "spawn ffmpeg +  socker servers to create mjpeg streams",
  "main": "",
  "bin": "./dist/index.js",
  "scripts": {
    "pkg": "pkg . --out-path ./dist/bin --targets node8-linux-x64",
    "build": "rimraf dist && tsc",
    "dev": "cross-env NODE_ENV=development nodemon --watch ./ --exec ts-node src/index.ts",
    "start": "node dist/index.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.18.2",
    "command-exists": "1.2.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "fluent-ffmpeg": "2.1.2",
    "fs-extra": "4.0.2",
    "lodash": "4.17.4",
    "morgan": "1.9.0",
    "nodemon": "1.12.1",
    "onvif": "0.5.3",
    "portscanner": "2.1.1",
    "serve-index": "1.9.1",
    "winston": "2.4.0",
    "winston-daily-rotate-file": "1.7.2",
    "ws": "7.2.1"
  },
  "devDependencies": {
    "@types/express": "4.0.37",
    "@types/fluent-ffmpeg": "2.1.5",
    "@types/fs-extra": "4.0.2",
    "@types/lodash": "4.14.78",
    "@types/node": "8.0.46",
    "@types/serve-index": "1.7.28",
    "@types/winston": "2.3.6",
    "@types/ws": "3.2.0",
    "commitizen": "2.9.6",
    "cross-env": "5.1.0",
    "ncp": "2.0.0",
    "pkg": "4.2.5",
    "rimraf": "2.6.2",
    "ts-node": "3.3.0",
    "typescript": "2.5.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
